{"ast":null,"code":"// import ClassicEditor from 'ckeditor5-build-classic-mathtype';\nimport ClassicEditor from \"ckeditor5-build-classic-mathtype\";\nimport { ref } from 'vue'; // import ClassicEditor from '@wiris/mathtype-ckeditor5'\n\nexport default {\n  name: 'LatexComponent',\n  props: ['isUpdate', 'content', 'isAnswer'],\n  watch: {\n    isUpdate() {\n      this.disabled = !this.isUpdate;\n    },\n\n    disabled() {\n      this.render = false;\n      this.editorConfig.toolbar = this.disabled ? {\n        items: []\n      } : {\n        items: ['Mathtype', \"bold\", \"italic\", \"imageUpload\", \"insertTable\"]\n      };\n      this.$nextTick(() => {\n        this.render = true;\n      });\n    },\n\n    editorData() {\n      this.$emit('update', this.editorData);\n    }\n\n  },\n\n  setup() {\n    const editor = ref(ClassicEditor);\n    return {\n      editor\n    };\n  },\n\n  // Lần đầu tiên phải mout cho disabled\n  mounted() {\n    this.disabled = !this.isUpdate;\n  },\n\n  data() {\n    return {\n      editorData: this.content,\n      // editorData: \"<a href='#' onclick='alert('XSS')'>Test</a>\",\n      editorConfig: {\n        toolbar: []\n      },\n      disabled: true,\n      render: true\n    };\n  },\n\n  methods: {\n    reRender() {\n      this.render = false;\n      this.$nextTick(() => {\n        this.render = true;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAYA;AACA,OAAOA,aAAP,MAA0B,kCAA1B;AACA,SAASC,GAAT,QAAoB,KAApB,C,CACA;;AACA,eAAe;EACXC,IAAI,EAAE,gBADK;EAEXC,KAAK,EAAE,CAAC,UAAD,EAAa,SAAb,EAAwB,UAAxB,CAFI;EAGXC,KAAK,EAAE;IACHC,QAAQ,GAAG;MACP,KAAKC,QAAL,GAAgB,CAAC,KAAKD,QAAtB;IACH,CAHE;;IAIHC,QAAQ,GAAG;MACP,KAAKC,MAAL,GAAc,KAAd;MACA,KAAKC,YAAL,CAAkBC,OAAlB,GAA4B,KAAKH,QAAL,GAAgB;QAAEI,KAAK,EAAE;MAAT,CAAhB,GAAgC;QACxDA,KAAK,EAAE,CAAC,UAAD,EAAa,MAAb,EACH,QADG,EACO,aADP,EACsB,aADtB;MADiD,CAA5D;MAIA,KAAKC,SAAL,CAAe,MAAM;QACjB,KAAKJ,MAAL,GAAc,IAAd;MACH,CAFD;IAGH,CAbE;;IAcHK,UAAU,GAAG;MACT,KAAKC,KAAL,CAAW,QAAX,EAAqB,KAAKD,UAA1B;IA