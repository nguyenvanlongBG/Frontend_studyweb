this.$emit('update', this.editorData)\r\n        }\r\n\r\n    },\r\n    setup() {\r\n        const editor = ref(ClassicEditor);\r\n        return {\r\n            editor\r\n        }\r\n    },\r\n    // Lần đầu tiên phải mout cho disabled\r\n    mounted() {\r\n        this.disabled = !this.isUpdate\r\n    },\r\n    data() {\r\n\r\n        return {\r\n            editorData: this.content,\r\n            // editorData: \"<a href='#' onclick='alert('XSS')'>Test</a>\",\r\n            editorConfig: {\r\n                toolbar: [],\r\n            },\r\n            disabled: true,\r\n            render: true\r\n        }\r\n    },\r\n    methods: {\r\n        reRender() {\r\n            this.render = false\r\n            this.$nextTick(() => {\r\n                this.render = true\r\n            })\r\n        }\r\n\r\n    },\r\n\r\n}\r\n</script>\r\n<style>\r\n.styleEditor {\r\n    /* margin-top: 5px; */\r\n    color: #222;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}