{"ast":null,"code":"import { getTests } from \"../services/test\";\nimport ModalCreateTest from \"../components/ModalCreateTest.vue\";\nimport { ref } from \"vue\";\nimport router from \"@/router\";\nexport default {\n  name: 'CompetitionsComponent',\n  components: {\n    ModalCreateTest\n  },\n\n  setup() {\n    const type = ref(0);\n    return {\n      type\n    };\n  },\n\n  data() {\n    return {\n      events: [{\n        id: 1,\n        name: \"Cuộc thi ai là thiên tài\"\n      }],\n      data: [],\n      modalCreateTest: {\n        visible: false\n      },\n      hasMyTests: false,\n      hasRole: true,\n      displayButton: true,\n      filter: {},\n      title: \"Tất cả bộ đề\"\n    };\n  },\n\n  mounted() {// this.getListTest()\n  },\n\n  methods: {\n    async getListTest() {\n      const response = await getTests(this.filter);\n      this.data = response.data;\n    },\n\n    getCompetition(id) {\n      router.push({\n        name: \"detailCompetition\",\n        params: {\n          id: id\n        }\n      });\n    },\n\n    createTest() {\n      this.modalCreateTest.visible = true;\n    },\n\n    getMyListTests() {\n      this.hasMyTests = true;\n      this.displayButton = false;\n      this.filter = {}; // this.filter.user_id = 2\n\n      this.getListTest();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,SAASA,QAAT,QAAyB,kBAAzB;AACA,OAAOC,eAAP,MAA4B,mCAA5B;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,MAAP,MAAmB,UAAnB;AAEA,eAAe;EACXC,IAAI,EAAE,uBADK;EAEXC,UAAU,EAAE;IACRJ;EADQ,CAFD;;EAKXK,KAAK,GAAG;IACJ,MAAMC,IAAG,GAAIL,GAAG,CAAC,CAAD,CAAhB;IACA,OAAO;MACHK;IADG,CAAP;EAGH,CAVU;;EAWXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,CAAC;QAAEC,EAAE,EAAE,CAAN;QAASN,IAAI,EAAE;MAAf,CAAD,CADL;MAEHI,IAAI,EAAE,EAFH;MAGHG,eAAe,EAAE;QACbC,OAAO,EAAE;MADI,CAHd;MAMHC,UAAU,EAAE,KANT;MAOHC,OAAO,EAAE,IAPN;MAQHC,aAAa,EAAE,IARZ;MASHC,MAAM,EAAE,EATL;MAUHC,KAAK,EAAE;IAVJ,CAAP;EAYH,CAxBU;;EAyBXC,OAAO,GAAG,CACN;EACH,CA3BU;;EA4BXC,OAAO,EAAE;IACL,MAAMC,WAAN,GAAoB;MAChB,MAAMC,QAAO,GAAI,MA