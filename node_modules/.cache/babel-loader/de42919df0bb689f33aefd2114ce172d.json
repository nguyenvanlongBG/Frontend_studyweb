  library.classList.toggle(\"active\");\n        var elements = document.querySelectorAll('.parent:not(#library)');\n        elements.forEach(element => {\n          element.classList.toggle(\"invisible\");\n        });\n      }, false);\n    },\n\n    choiceAnswer() {}\n\n  }\n};","map":{"version":3,"mappings":"AAaA;AACA,SAASA,iBAAT,QAAkC,sBAAlC;AACA,OAAOC,aAAP,MAA0B,4BAA1B;AACA,OAAOC,iBAAP,MAA8B,kCAA9B;AACA,SAASC,GAAT,QAAoB,iBAApB;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,eAAe;EACbC,IAAI,EAAE,gBADO;EAEbC,UAAU,EAAE;IACVN,aADU;IAEVC;EAFU,CAFC;;EAMbM,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIN,GAAG,CAAC,KAAD,CAArB;IACA,MAAMO,SAAQ,GAAIP,GAAG,CAAC,IAAD,CAArB;IACA,MAAMQ,SAAQ,GAAIR,GAAG,CAAC,EAAD,CAArB;IACA,MAAMS,MAAK,GAAIT,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMU,MAAK,GAAIV,GAAG,CAAC,IAAD,CAAlB;IACA,MAAMW,eAAc,GAAIX,GAAG,CAAC,KAAD,CAA3B;IACA,MAAMY,QAAO,GAAIZ,GAAG,CAAC,EAAD,CAApB;IACA,MAAMa,UAAS,GAAIb,GAAG,CAAC,CAAD,CAAtB;IACA,MAAMc,UAAS,GAAId,GAAG,CAAC,IAAD,CAAtB;IACA,OAAO;MACLM,SADK;MAELK,eAFK;MAGLJ,SAHK;MAILC,SAJK;MAKLC,MALK;MAMLG,QANK;MAOLC,UAPK;MAQLC,UARK;MASLJ;IATK,CAAP;EAWD,CA3BY;;EA6BbK,OAAO,GAAG;IACR;IACA,KAAKC,aAAL;EACD,CAhCY;;EAiCbC,OAAO,EAAE;IACPC,WAAW,CAACC,EAAD,EAAK;MACdC,OAAO,CAACC,GAAR,CAAYF,EAAZ;MACA,KAAKL,UAAL,GAAkBK,EAAlB;MACAC,OAAO,CAACC,GAAR,CAAY,KAAKP,UAAjB;MACA,KAAKH,eAAL,GAAuB,IAAvB;IACD,CANM;;IAOPW,aAAa,GAAG;MACd,KAAKX,eAAL,GAAuB,KAAvB;MACA,KAAKG,UAAL,GAAkB,IAAlB;IACD,CAVM;;IAWPS,cAAc,CAACC,CAAD,EAAIC,GAAJ,EAAS;MACrBL,OAAO,CAACC,GAAR,CAAYI,GAAZ;MACA,IAAIC,KAAI,GAAIF,CAAC,CAACG,MAAF,CAASD,KAAT,IAAkBF,CAAC,CAACI,YAAF,CAAeF,KAA7C;MACA,IAAI,CAACA,KAAK,CAACG,MAAX,EAAmB;MAEnB5B,KAAK,CAAC6B,IAAN,CAAW,gDAAX,EAA6D,EAA7D,EACGC,IADH,CACQC,QAAO,IAAK;QAChBZ,OAAO,CAACC,GAAR,CAAYW,QAAZ;MACD,CAHH,EAIGC,KAJH,CAISD,QAAO,IAAK;QACjBZ,OAAO,CAACC,GAAR,CAAYW,QAAZ;MACD,CANH;IAOF,CAvBO;;IAyBP,MAAMhB,aAAN,GAAsB;MACpB,MAAMkB,iBAAgB,GAAI,MAAMrC,iBAAiB,EAAjD;MACA,KAAKW,SAAL,GAAiB0B,iBAAiB,CAACC,IAAlB,CAAuB3B,SAAxC;IACD,CA5BM;;IA6BP4B,QAAQ,GAAG;MACT,IAAIC,OAAM,GAAIC,QAAQ,CAACC,cAAT,CAAwB,