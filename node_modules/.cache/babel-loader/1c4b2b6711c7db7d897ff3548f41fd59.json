{"ast":null,"code":"import { getTests } from \"../services/test\";\nimport ModalCreateTest from \"../components/ModalCreateTest.vue\";\nimport TestComponent from \"../components/Test/TestComponent.vue\";\nexport default {\n  name: 'TestsComponent',\n  components: {\n    ModalCreateTest,\n    TestComponent\n  },\n\n  data() {\n    return {\n      data: [],\n      modalCreateTest: {\n        visible: false\n      },\n      hasMyTests: false,\n      hasRole: true,\n      displayButton: true,\n      filter: {},\n      title: \"Tất cả bộ đề\"\n    };\n  },\n\n  mounted() {\n    this.getListTest();\n  },\n\n  methods: {\n    async getListTest() {\n      const response = await getTests(this.filter);\n      this.data = response;\n    },\n\n    createTest() {\n      this.modalCreateTest.visible = true;\n    },\n\n    getMyListTests() {\n      this.hasMyTests = true;\n      this.displayButton = false;\n      this.filter = {}; // this.filter.user_id = 2\n\n      this.getListTest();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,QAAT,QAAyB,kBAAzB;AACA,OAAOC,eAAP,MAA4B,mCAA5B;AACA,OAAOC,aAAP,MAA0B,sCAA1B;AAEA,eAAe;EACbC,IAAI,EAAE,gBADO;EAEbC,UAAU,EAAE;IACVH,eADU;IAEVC;EAFU,CAFC;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,eAAe,EAAE;QACfC,OAAO,EAAE;MADM,CAFZ;MAKLC,UAAU,EAAE,KALP;MAMLC,OAAO,EAAE,IANJ;MAOLC,aAAa,EAAE,IAPV;MAQLC,MAAM,EAAE,EARH;MASLC,KAAK,EAAE;IATF,CAAP;EAWD,CAlBY;;EAmBbC,OAAO,GAAG;IACR,KAAKC,WAAL;EACD,CArBY;;EAsBbC,OAAO,EAAE;IACP,MAAMD,WAAN,GAAoB;MAClB,MAAME,QAAO,GAAI,MAAMhB,QAAQ,CAAC,KAAKW,MAAN,CAA/B;MACA,KAAKN,IAAL,GAAYW,QAAZ;IACD,CAJM;;IAKPC,UAAU,GAAG;MACX,KAAKX,eAAL,CAAqBC,OAArB,GAA+B,IAA/B;IACD,CAPM;;IAQPW,cAAc,GAAG;MACf,KAAKV,UAAL,GAAkB,IAAlB;MACA,KAAKE,aAAL,GAAqB,KAArB;MACA,KAAKC,MAAL,GAAc,EAAd,CAHe,CAIf;;MACA,KAAKG,WAAL;IACF;;EAdO;AAtBI,CAAf","names":["getTests","ModalCreateTest","TestComponent","name","components","data","modalCreateTest","visible","hasMyTests","hasRole","displayButton","filter","title","mounted","getListTest","methods","response","createTest","getMyListTests"],"sourceRoot":"","sources":["G:\\Dự án Web học tập\\studyweb\\src\\components\\TestsComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <!-- Giao diện chọn các bộ đề -->\r\n    <ModalCreateTest v-if=\"modalCreateTest.visible\" @close=\"modalCreateTest.visible = false\">\r\n\r\n    </ModalCreateTest>\r\n    <div class=\"header-tests\">\r\n      <h1 class=\"myListTestTitle\">{{ title }}</h1>\r\n      <div class=\"list-tool-button-test\">\r\n        <button class=\"tool-button-test\" @click=\"createTest\" v-if=\"hasRole\" id=\"create\">\r\n          Tạo đề thi\r\n        </button>\r\n        <button class=\"tool-button-test\" @click=\"getMyListTests\" :disabled=\"hasRole\" id=\"doingTest\">\r\n          Đang thi\r\n        </button>\r\n        <button class=\"tool-button-test\" @click=\"getMyListTests\" :disabled=\"displayButton\" id=\"history\">\r\n          Lịch sử\r\n        </button>\r\n        <button class=\"tool-button-test\" @click=\"getMyListTests\" id=\"myListTest\">\r\n          Bộ đề của tôi\r\n        </button>\r\n        <button class=\"tool-button-test\" @click=\"getMyListTests\" :disabled=\"!hasMyTests\" id=\"all\">\r\n          Tất cả\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"grid-container\">\r\n      <TestComponent :test=\"test\" v-for=\"test in data\" :key=\"test.id\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getTests } from \"../services/test\"\r\nimport ModalCreateTest from \"../components/ModalCreateTest.vue\"\r\nimport TestComponent from \"../components/Test/TestComponent.vue\"\r\n\r\nexport default {\r\n  name: 'TestsComponent',\r\n  components: {\r\n    ModalCreateTest,\r\n    TestComponent,\r\n  },\r\n  data() {\r\n    return {\r\n      data: [],\r\n      modalCreateTest: {\r\n        visible: false,\r\n      },\r\n      hasMyTests: false,\r\n      hasRole: true,\r\n      displayButton: true,\r\n      filter: {},\r\n      title: \"Tất cả bộ đề\"\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getListTest()\r\n  },\r\n  methods: {\r\n    async getListTest() {\r\n      const response = await getTests(this.filter)\r\n      this.data = response\r\n    },\r\n    createTest() {\r\n      this.modalCreateTest.visible = true\r\n    },\r\n    getMyListTests() {\r\n      this.hasMyTests = true\r\n      this.displayButton = false\r\n      this.filter = {}\r\n      // this.filter.user_id = 2\r\n      this.getListTest()\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style>\r\n.container {\r\n  margin: 0 15%;\r\n  position: relative;\r\n\r\n}\r\n\r\n.myListTestTitle {\r\n  margin-top: 5px;\r\n  margin-bottom: 5px;\r\n  margin-left: 15px;\r\n}\r\n\r\n\r\n\r\n.list-tool-button-test {\r\n  display: flex;\r\n}\r\n\r\n.tool-button-test {\r\n  margin-left: 2px;\r\n  margin-top: 2px;\r\n  height: 35px;\r\n  box-shadow: 0 8px #999;\r\n  border-radius: 5px;\r\n  border: 2px solid #ea4f4c;\r\n  background-color: #222;\r\n  color: white;\r\n  text-align: center;\r\n  justify-content: center;\r\n  text-decoration: none;\r\n}\r\n\r\n.tool-button-test:active {\r\n\r\n  box-shadow: 0 5px #666;\r\n  transform: translateY(4px);\r\n}\r\n\r\n.share {\r\n  font-size: 22px;\r\n}\r\n\r\n.grid-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  padding: 10px;\r\n  padding-top: 0px;\r\n  margin-top: 2px;\r\n  box-sizing: border-box;\r\n  border-radius: 20px;\r\n\r\n}\r\n\r\n.grid-item {\r\n  box-sizing: border-box;\r\n  background-color: #222;\r\n  border: 2px solid #ea4f4c;\r\n  width: 24.5%;\r\n  color: white;\r\n  border-radius: 10px;\r\n  cursor: pointer;\r\n  padding: 20px;\r\n  padding-top: 10px;\r\n  font-size: 20px;\r\n  text-align: center;\r\n  margin-left: 2px;\r\n  margin-top: 5px;\r\n  text-decoration: none;\r\n}\r\n\r\n.header-exam {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}